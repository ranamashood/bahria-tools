{
  "manifest_version": 3,
  "name": "Bahria Tools",
  "description": "Tools used for Bahria University CMS",
  "version": "0.0.1",
  "action": {
    "default_popup": "popup.html"
  },
  "permissions": ["activeTab", "tabs", "storage"],
  "content_scripts": [
    {
      "matches": ["https://cms.bahria.edu.pk/*"],
      "css": ["content.css"],
      "js": ["content.js"]
    }
  ]
}
